{
  "name": "software-testing",
  "version": "1.0.0",
  "description": "Jest vs React Testing Library",
  "main": "index.js",
  "scripts": {
    "update-toc": "npx markdown-toc -i README.md --bullets \"-\"",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "husky": "^9.0.9",
    "prettier": "^3.2.5"
  },
  "husky": {
    "hooks": {
      "applypatch-msg": "echo \"[Husky] applypatch-msg\"",
      "pre-applypatch": "echo \"[Husky] pre-applypatch\"",
      "post-applypatch": "echo \"[Husky] post-applypatch\"",
      "pre-commit": "echo \"[Husky] pre-commit\""
    }
  },
  "dependencies": {
    "markdown-toc": "^1.2.0"
  }
}
